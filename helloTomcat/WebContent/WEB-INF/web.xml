<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:web="http://xmlns.jcp.org/xml/ns/javaee">

  <servlet>
    <servlet-name>FoodFormRead</servlet-name>
    <servlet-class>com.tomcatTest.FoodFormRead</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FoodFormRead</servlet-name>
    <url-pattern>/FoodFormRead</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FoodMenuOperation</servlet-name>
    <servlet-class>com.tomcatTest.FoodMenuOperation</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FoodMenuOperation</servlet-name>
    <url-pattern>/FoodMenuOperation</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>DeleteFood</servlet-name>
    <servlet-class>com.tomcatTest.DeleteFood</servlet-class>
  </servlet>
    <servlet-mapping>
    <servlet-name>DeleteFood</servlet-name>
    <url-pattern>/DeleteFood</url-pattern>
  </servlet-mapping>
